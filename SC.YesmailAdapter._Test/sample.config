<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <appSettings>
    <add key="YesMailUrl" value="https://services.yesmail.com/enterprise/composite/subscribeAndSend"/>
    <add key="YesMailDomain" value="kclarkusfc"/>
    <add key="YesMailUserName" value="kceapi"/>
    <add key="YesMailPassword" value="K8c7e6a"/>
    <add key="YesMailProxyUrl" value="proxy.kcc.com"/>
    <add key="YesMailProxyPort" value="80"/>
    <add key="YesMailProxyUserName" value="USRNAP23"/>
    <add key="YesMailProxyPassword" value="kcc@1238"/>
    <add key="YesMailProxyDomain" value="kcus"/>
    <add key="yesmailReturnUrl" value="http://handhygienedev.studiocom.com/90Day"/>
    <add key="YesMailUseProxy" value="false"/>
    <add key="enableSecuritySec" value="false"/>
    <add key="enableWcfSecurity" value="false"/>

    <add key="yesmailER1_DefaultPackReceiver" value="1256210"/>
  </appSettings>
  <log4net debug="false">
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="Application.log"/>
      <appendToFile value="false"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="INFO"/>
        <foreColor value="White"/>
        <backColor value="Red, HighIntensity"/>
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <root>
      <priority value="ALL"/>
      <appender-ref ref="FileAppender"/>
      <appender-ref ref="ColoredConsoleAppender"/>
      <appender-ref ref="ConsoleAppender"/>
    </root>
    <category name="yesmailTriggerEmail">
      <priority value="ALL"/>
    </category>
  </log4net>
  <startup>

    <supportedRuntime version="v2.0.50727"/>
  </startup>
</configuration>
